<?xml version="1.0"?>

<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>VL.Ollama</id>
    <version>0.0.1</version>
    <title>VL.Ollama</title>
    <authors>sebl</authors>
    <projectUrl>https://github.com/schnellebuntebilder/VL.Ollama</projectUrl>
    <repository type="git" url="https://github.com/schnellebuntebilder/VL.Ollama" />
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>a VL Wrapper for https://github.com/awaescher/OllamaSharp</description>
    <tags>VL, VVVV, Ollama, llm, machine learning, ai</tags>
    <license type="expression">LGPL-3.0-only</license>
    <icon>icon\nugeticon.png</icon>
    <readme>docs\README.md</readme>
    <dependencies>
        <dependency id="OllamaSharp" version="5.0.2" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\csharp\**" target="csharp\" exclude="" /> 
    <file src="..\*.vl" target="" exclude="" />
    <file src="..\help\**" target="help\" exclude="" />
    <file src="nugeticon.png" target="icon\" />
    <file src="..\README.md" target="docs\" />
  </files>
</package>